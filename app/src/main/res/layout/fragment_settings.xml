<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/recycleview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingsFragment">

    <!-- TODO: Update blank fragment layout -->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="425dp"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="101dp"
            android:layout_height="89dp"
            android:layout_marginTop="28dp"
            android:src="@drawable/faces"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.984"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="148dp"
            android:layout_height="46dp"
            android:layout_marginTop="88dp"
            android:text="Settings"
            android:textColor="#634F47"
            android:textSize="35dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.137"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView4"
            android:layout_width="355dp"
            android:layout_height="76dp"
            android:layout_marginTop="32dp"
            app:cardBackgroundColor="#EDDCD5"
            app:cardCornerRadius="20dp"
            android:layout_marginStart="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3">

            <ImageView
                android:layout_width="63dp"
                android:layout_height="53dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:rotation="90"
                android:src="@drawable/baseline_key_24" />

            <TextView
                android:layout_width="141dp"
                android:layout_height="40dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="17dp"
                android:text="Account"
                android:textColor="#784936"
                android:textSize="27dp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView5"
            android:layout_width="355dp"
            android:layout_height="76dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="#EDDCD5"
            app:cardCornerRadius="20dp"
            android:layout_marginStart="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView4">

            <ImageView
                android:layout_width="63dp"
                android:layout_height="53dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/outline_lock_24" />

            <TextView
                android:layout_width="141dp"
                android:layout_height="40dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="17dp"
                android:text="Privacy"
                android:textColor="#784936"
                android:textSize="27dp"
                android:textStyle="bold" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView6"
            android:layout_width="355dp"
            android:layout_height="76dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="#EDDCD5"
            app:cardCornerRadius="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView5">

            <ImageView
                android:layout_width="63dp"
                android:layout_height="53dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/outline_notifications_none_24" />

            <TextView
                android:layout_width="159dp"
                android:layout_height="40dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="17dp"
                android:text="Notifications"
                android:textColor="#784936"
                android:textSize="27dp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView7"
            android:layout_width="355dp"
            android:layout_height="76dp"
            android:layout_marginTop="20dp"
            android:layout_marginStart="20dp"
            app:cardBackgroundColor="#EDDCD5"
            app:cardCornerRadius="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView6">

            <ImageView
                android:layout_width="63dp"
                android:layout_height="53dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/round_help_outline_24" />

            <TextView
                android:layout_width="141dp"
                android:layout_height="40dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="17dp"
                android:text="Help"
                android:textColor="#784936"
                android:textSize="27dp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="355dp"
            android:layout_height="76dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="#EDDCD5"
            app:cardCornerRadius="20dp"
            android:layout_marginStart="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView7">

            <ImageView
                android:layout_width="63dp"
                android:layout_height="53dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/baseline_share_24" />

            <TextView
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="17dp"
                android:text="Invite friends"
                android:textColor="#784936"
                android:textSize="27dp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>